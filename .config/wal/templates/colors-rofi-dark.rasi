    alt-secondary: rgba({color2.rgb}, 0.5);
    alt-tertiary: rgba({color3.rgb}, 0.5);
    alt-four: rgba({color1.rgb}, 0.5);
    alt-five: rgba({color5.rgb}, 0.5);
    alt-urgent: rgba({color6.rgb}, 0.5);
}}
