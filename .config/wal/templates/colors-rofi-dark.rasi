* {{
    background: rgba({background.rgb}, 0.5);
    background-select: rgba({foreground.rgb}, 0.075);
    text-color: {foreground};
    selected-text-color: {color2};
    primary: {color4};
    secondary: {color2};
    tertiary: {color3};
    four: {color1};
    five: {color5};
    urgent: {color6};
